<Window x:Class="Main.Integration.IngSetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Main.Integration"
        mc:Ignorable="d"
        Title="联动设置" Height="450" Width="400" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <CheckBox Name="cbSF">二层台</CheckBox>
            <CheckBox Name="cbDR">钻台面</CheckBox>
            <CheckBox Name="cbSIR">铁钻工</CheckBox>
            <Button Click="Button_Click">确定</Button>
        </StackPanel>
    </Grid>
</Window>
