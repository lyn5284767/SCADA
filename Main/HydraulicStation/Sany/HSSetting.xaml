                                            <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnLeftCatHeadRetract" FontSize="18" Width="80" Height="40">缩</Button>
                                            <Button Name="btnLeftCatHeadClose"  FontSize="18" Width="80" Height="40" Click="BtnLeftCatClose_Click">关</Button>
                                            <Button Name="btnLeftCatHeadReach" FontSize="18" Width="80" Height="40">伸</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="0" Grid.Column="1" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="右猫头:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <TextBlock Name="tbRightCatHead" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                            <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnRightCatHeadRetract" FontSize="18" Width="80" Height="40">缩</Button>
                                            <Button Name="btnRightCatHeadClose" FontSize="18" Width="80" Height="40" Click="BtnRightCatClose_Click">关</Button>
                                            <Button Name="btnRightCatHeadReach" FontSize="18" Width="80" Height="40">伸</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <!--<StackPanel Grid.Column="0" Grid.Row="1" VerticalAlignment="Center">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <Button Name="btnIron" FontSize="18" Width="80" Height="50" Click="BtnIron_Click">铁钻工</Button>
                                    <Button Name="btnIronClose" FontSize="18" Width="80" Height="50" Click="BtnIronClose_Click">关闭</Button>
                                    <Button Name="btnTong" FontSize="18" Width="80" Height="50" Click="BtnTong_Click">大钳</Button>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBlock FontSize="18" Text="铁钻工/大钳:"></TextBlock>
                                    <TextBlock Name="tbIronTongs" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                    <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                </StackPanel>
                            </StackPanel>-->
                                <Border Grid.Row="1" Grid.Column="0" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="主泵卸荷:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <TextBlock Name="tbMainPumpUnload" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                            <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnMainPumpUnloadOpen" FontSize="18" Width="120" Height="50" Click="btnMainPumpUnloadOpen_Click">打开</Button>
                                            <Button Name="btnMainPumpUnloadClose" FontSize="18" Width="120" Height="50" Click="btnMainPumpUnloadClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="1" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="卡瓦卸荷:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <TextBlock Name="tbKavaUnload" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                            <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnKavaUnloadOpen" FontSize="18" Width="120" Height="40" Click="btnKavaUnloadOpen_Click">打开</Button>
                                            <Button Name="btnKavaUnloadClose" FontSize="18" Width="120" Height="40" Click="btnKavaUnloadClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="2" Grid.Column="0" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="井架平移:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <TextBlock Name="tbWellMove" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                            <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnWellMoveOpen" FontSize="18" Width="120" Height="40" Click="btnWellMoveOpen_Click">打开</Button>
                                            <Button Name="btnWellMoveClose" FontSize="18" Width="120" Height="40" Click="btnWellMoveClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="2" Grid.Column="1" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="备用油源1:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <!--<TextBlock Name="tbWellMove" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                        <TextBlock FontSize="18" Text="Mpa"></TextBlock>-->
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnSpareOneOpen" FontSize="18" Width="120" Height="40" Click="btnSpareOneOpen_Click">打开</Button>
                                            <Button Name="btnSpareOneClose" FontSize="18" Width="120" Height="40" Click="btnSpareOneClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="3" Grid.Column="0" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="铁钻工:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <TextBlock Name="tbIron" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                            <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnIronOpen" FontSize="18" Width="120" Height="40" Click="btnIronOpen_Click">打开</Button>
                                            <Button Name="btnIronClose" FontSize="18" Width="120" Height="40" Click="BtnIronClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="3" Grid.Column="1" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="大钳:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <TextBlock Name="tbTongs" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                            <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnTongsOpen" FontSize="18" Width="120" Height="40" Click="btnTongsOpen_Click">打开</Button>
                                            <Button Name="btnTongsClose" FontSize="18" Width="120" Height="40" Click="btnTongsClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="4" Grid.Column="0" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="钻台面:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <TextBlock Name="tbDR" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                            <TextBlock FontSize="18" Text="Mpa"></TextBlock>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnDROpen" FontSize="18" Width="120" Height="40" Click="btnDROpen_Click">打开</Button>
                                            <Button Name="btnDRClose" FontSize="18" Width="120" Height="40" Click="btnDRClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="4" Grid.Column="1" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
