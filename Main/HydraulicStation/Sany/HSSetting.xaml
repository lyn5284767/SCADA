                                            <!--<TextBlock Name="tbDR" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                        <TextBlock FontSize="18" Text="Mpa"></TextBlock>-->
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnSpareTwoOpen" FontSize="18" Width="120" Height="40" Click="btnSpareTwoOpen_Click">打开</Button>
                                            <Button Name="btnSpareTwoClose" FontSize="18" Width="120" Height="40" Click="btnSpareTwoClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="5" Grid.Column="0" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="缓冲臂:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <!--<TextBlock Name="tbArm" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                        <TextBlock FontSize="18" Text="Mpa"></TextBlock>-->
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnArmOpen" FontSize="18" Width="120" Height="40" Click="btnArmOpen_Click">打开</Button>
                                            <Button Name="btnArmClose" FontSize="18" Width="120" Height="40" Click="btnArmClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="5" Grid.Column="1" BorderBrush="White" BorderThickness="1">
                                    <StackPanel VerticalAlignment="Center">
                                        <StackPanel Orientation="Horizontal" >
                                            <TextBlock FontSize="18" Text="备用油源3:" FontWeight="Bold" Foreground="Black"></TextBlock>
                                            <!--<TextBlock Name="tbArm" FontSize="18" Width="60" Text="8888.8"></TextBlock>
                                        <TextBlock FontSize="18" Text="Mpa"></TextBlock>-->
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Button Name="btnSpareThreeOpen" FontSize="18" Width="120" Height="40" Click="btnSpareThreeOpen_Click">打开</Button>
                                            <Button Name="btnSpareThreeClose" FontSize="18" Width="120" Height="40" Click="btnSpareThreeClose_Click">关闭</Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <!--<StackPanel Grid.Column="1" Grid.Row="1" VerticalAlignment="Center">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <Button Name="btnDF" FontSize="18" Width="80" Height="50" Click="BtnDF_Click">钻台面</Button>
                                    <Button Name="btnDFClose" FontSize="18" Width="80" Height="50" Click="BtnDFClose_Click">关闭</Button>
