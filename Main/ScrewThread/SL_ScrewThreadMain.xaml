                        </Border>
                        <Border Grid.Row="2" Grid.Column="3">
                            <TextBlock Name="tbMousePos" Text="XX" Foreground="#008AFF"></TextBlock>
                        </Border>
                        <Border Grid.Row="3" Grid.Column="3" Background="#DDE0E5">
                            <TextBlock Name="tbPointPos" Text="XX" Foreground="#008AFF"></TextBlock>
                        </Border>
