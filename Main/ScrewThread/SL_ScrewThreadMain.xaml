                        </Border>
                        <Border Grid.Row="1" Grid.Column="1" Background="#DDE0E5">
                            <TextBlock Name="tbWellAngle" Text="XX" Foreground="#008AFF"></TextBlock>
                        </Border>
                        <Border Grid.Row="2" Grid.Column="1">
                            <TextBlock Name="tbMouseAngle" Text="XX" Foreground="#008AFF"></TextBlock>
                        </Border>
                        <Border Grid.Row="3" Grid.Column="1" Background="#DDE0E5">
                            <TextBlock Name="tbPointAngle" Text="XX" Foreground="#008AFF"></TextBlock>
                        </Border>
                        <Border Grid.Row="4" Grid.Column="1">
